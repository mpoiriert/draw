{
    "name": "draw/tester",
    "description": "Library base on phpunit to test your data with a fluent interface.",
    "license": "MIT",
    "type": "library",
    "keywords": ["draw", "test", "http", "phpunit"],
    "authors": [
        {
            "name": "Martin Poirier Theoret",
            "email": "mpoiriert@gmail.com"
        }
    ],
    "require": {
        "ext-simplexml": "*",
        "draw/core": "^0.10",
        "guzzlehttp/psr7": "^1.8 || ^2.0",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "psr/http-message": "^1.0",
        "symfony/property-access": "^5.4.3"
    },
    "require-dev": {
        "doctrine/orm": "^2.11",
        "pdeans/http": "^1.1",
        "symfony/console": "^5.4.3",
        "symfony/dependency-injection": "^5.4.3"
    },
    "suggest": {
        "pdeans/http": "If you wan to use the curl http client",
        "symfony/cache": "If you want to use the orm setup tool",
        "symfony/dependency-injection": "If you want to test extension/configuration"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Component\\Tester\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10-dev"
        }
    }
}
