{
    "name": "draw/messenger-bundle",
    "description": "",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["symfony", "messenger", "bundle"],
    "require": {
        "draw/contracts": "^0.8",
        "draw/messenger": "^0.8",
        "symfony/css-selector": "^5.4.3",
        "symfony/framework-bundle": "^5.4.3",
        "symfony/mailer": "^5.4.3",
        "symfony/translation": "^5.4.3"
    },
    "require-dev": {
        "ext-pcntl": "*",
        "draw/tester-bundle": "^0.8",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "sonata-project/admin-bundle": "^4.8",
        "sonata-project/doctrine-orm-admin-bundle": "^4.2"
    },
    "suggest": {
        "ext-pcntl": "To allow message broker to catch signal",
        "sonata-project/admin-bundle": "If you want to display doctrine message via sonata admin",
        "sonata-project/doctrine-orm-admin-bundle": "Sonata integration only support orm"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Bundle\\MessengerBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
