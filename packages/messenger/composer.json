{
    "name": "draw/messenger",
    "description": "Symfony messenger transport offering more option (base on doctrine transport)",
    "license": "MIT",
    "type": "library",
    "keywords": ["draw", "component", "messenger", "symfony", "doctrine"],
    "authors": [
        {
            "name": "Martin Poirier Theoret",
            "email": "mpoiriert@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "doctrine/dbal": "^3.3",
        "doctrine/persistence": "^2.2",
        "draw/contracts": "^1.0",
        "symfony/doctrine-bridge": "^5.4.3",
        "symfony/doctrine-messenger": "^5.4.3",
        "symfony/event-dispatcher": "^5.4.3",
        "symfony/mailer": "^5.4.3",
        "symfony/messenger": "^5.4.3",
        "symfony/process": "^5.4.3",
        "symfony/translation": "^5.4.3",
        "ramsey/uuid": "^4.2"
    },
    "require-dev": {
        "ext-pcntl": "*",
        "draw/tester-bundle": "^1.0",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "psr/log": "@stable",
        "sonata-project/admin-bundle": "^4.8",
        "sonata-project/doctrine-orm-admin-bundle": "^4.2"
    },
    "suggest": {
        "ext-pcntl": "To allow message broker to catch signal",
        "draw/framework-extra-bundle": "To integrate in Symfony",
        "sonata-project/admin-bundle": "If you want to display doctrine message via sonata admin",
        "sonata-project/doctrine-orm-admin-bundle": "Sonata integration only support orm"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Component\\Messenger\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
