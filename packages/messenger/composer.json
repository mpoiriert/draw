{
    "name": "draw/messenger",
    "description": "Symfony messenger transport offering more option (base on doctrine transport)",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "draw",
        "component",
        "messenger",
        "symfony",
        "doctrine"
    ],
    "authors": [
        {
            "name": "Martin Poirier Theoret",
            "email": "mpoiriert@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "doctrine/dbal": "^3.3",
        "doctrine/persistence": "^2.2 || ^3.0",
        "draw/contracts": "^0.29",
        "draw/core": "^0.29",
        "symfony/doctrine-bridge": "^6.4.0",
        "symfony/doctrine-messenger": "^6.4.0",
        "symfony/event-dispatcher": "^6.4.0",
        "symfony/http-foundation": "^6.4.0",
        "symfony/messenger": "^6.4.0",
        "symfony/process": "^6.4.0",
        "symfony/routing": "^6.4.0",
        "symfony/translation": "^6.4.0",
        "ramsey/uuid": "^4.2"
    },
    "require-dev": {
        "ext-pcntl": "*",
        "colinodell/psr-testlogger": "^1.1",
        "doctrine/orm": "^2.11",
        "draw/dependency-injection": "^0.29",
        "draw/tester": "^0.29",
        "phpunit/phpunit": "^11.3",
        "psr/log": "^3"
    },
    "suggest": {
        "ext-pcntl": "To allow message broker to catch signal",
        "draw/framework-extra-bundle": "To integrate in Symfony"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Component\\Messenger\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.29-dev"
        }
    }
}
