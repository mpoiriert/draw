{
    "name": "draw/sonata-import-bundle",
    "description": "Generic customisable import system for sonata",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "draw",
        "bundle",
        "symfony"
    ],
    "authors": [
        {
            "name": "Martin Poirier Theoret",
            "email": "mpoiriert@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "draw/sonata-extra-bundle": "^0.12",
        "sonata-project/admin-bundle": "^4.8",
        "sonata-project/doctrine-orm-admin-bundle": "^4.2",
        "symfony/event-dispatcher": "^6.4.0",
        "symfony/notifier": "^6.4.0",
        "symfony/validator": "^6.4.0"
    },
    "require-dev": {
        "draw/tester": "^0.12",
        "knplabs/doctrine-behaviors": "*",
        "phpunit/phpunit": "^11.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Bundle\\SonataImportBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.12-dev"
        }
    }
}
