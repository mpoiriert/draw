{
    "name": "draw/sonata-integration-bundle",
    "description": "Integrate compatible draw packages in Sonata admin",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["symfony", "sonata"],
    "require": {
        "php": "^7.4",
        "draw/framework-extra-bundle": "^1.0",
        "draw/sonata-extra-bundle":  "^1.0",
        "symfony/framework-bundle": "^5.4.3",
        "symfony/expression-language": "^5.4.3",
        "sonata-project/admin-bundle": "^4.8",
        "sonata-project/doctrine-orm-admin-bundle": "^4.2"
    },
    "require-dev": {
        "draw/messenger": "^1.0",
        "draw/tester": "^1.0",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Bundle\\SonataIntegrationBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
