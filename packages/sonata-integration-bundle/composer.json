{
    "name": "draw/sonata-integration-bundle",
    "description": "Integrate compatible draw packages in Sonata admin",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "symfony",
        "sonata"
    ],
    "require": {
        "php": ">=8.2",
        "draw/framework-extra-bundle": "^0.26",
        "draw/sonata-extra-bundle": "^0.26",
        "symfony/framework-bundle": "^6.4.0",
        "symfony/expression-language": "^6.4.0",
        "sonata-project/admin-bundle": "^4.8",
        "sonata-project/doctrine-orm-admin-bundle": "^4.2",
        "yokai/sonata-workflow": "^0.7.0"
    },
    "require-dev": {
        "draw/application": "^0.26",
        "draw/console": "^0.26",
        "draw/messenger": "^0.26",
        "draw/security": "^0.26",
        "draw/tester": "^0.26",
        "draw/user-bundle": "^0.26",
        "firebase/php-jwt": "^6.1",
        "phpunit/phpunit": "^11.3",
        "scheb/2fa-bundle": "^7.6",
        "scheb/2fa-email": "^7.6",
        "endroid/qr-code": "^6.0",
        "scheb/2fa-totp": "^7.6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Bundle\\SonataIntegrationBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.26-dev"
        }
    }
}
