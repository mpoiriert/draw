{
    "name": "draw/console",
    "description": "This component allow to command execution in the database.",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "draw",
        "console"
    ],
    "require": {
        "doctrine/dbal": "^3.3",
        "draw/core": "^0.35",
        "psr/log": "^3",
        "ramsey/uuid": "^4.2",
        "sensiolabs/ansi-to-html": "^1.2",
        "symfony/event-dispatcher": "^6.4.0",
        "symfony/console": "^6.4.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.11",
        "draw/dependency-injection": "^0.35",
        "draw/tester": "^0.35",
        "phpunit/phpunit": "^11.3 || ^12.0",
        "symfony/cache": "^6.4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Component\\Console\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.35-dev"
        }
    }
}
