{
    "name": "draw/doctrine-extra",
    "description": "Log related tool, mainly for monolog",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "draw",
        "log",
        "monolog"
    ],
    "authors": [
        {
            "name": "Martin Poirier Theoret",
            "email": "mpoiriert@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "doctrine/orm": "^2.11",
        "doctrine/dbal": "^3.3",
        "doctrine/collections": "^1.0",
        "doctrine/data-fixtures": "^1.5",
        "draw/dependency-injection": "^0.10.45",
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\DoctrineExtra\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.11-dev"
        }
    }
}
