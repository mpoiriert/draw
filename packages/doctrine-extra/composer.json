{
    "name": "draw/doctrine-extra",
    "description": "Log related tool, mainly for monolog",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "draw",
        "log",
        "monolog"
    ],
    "authors": [
        {
            "name": "Martin Poirier Theoret",
            "email": "mpoiriert@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "doctrine/orm": "^2.11",
        "doctrine/dbal": "^3.3",
        "doctrine/collections": "^1.0",
        "doctrine/data-fixtures": "^2.0",
        "draw/dependency-injection": "^0.16",
        "draw/graphviz": "^0.16",
        "phpunit/phpunit": "^11.3"
    },
    "suggest": {
        "draw/graphviz": "To generate graph of the database schema"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\DoctrineExtra\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.16-dev"
        }
    }
}
