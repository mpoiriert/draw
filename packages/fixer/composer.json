{
    "name": "draw/fixer",
    "description": "Customer rules for php-cs-fixer",
    "license": "MIT",
    "type": "library",
    "keywords": ["draw", "php-cs-fixer"],
    "authors": [
        {
            "name": "Martin Poirier Theoret",
            "email": "mpoiriert@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "friendsofphp/php-cs-fixer": "^3.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Fixer\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10-dev"
        }
    }
}
