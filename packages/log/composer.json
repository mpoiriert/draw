{
    "name": "draw/log",
    "description": "Log related tool, mainly for monolog",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "draw",
        "log",
        "monolog"
    ],
    "authors": [
        {
            "name": "Martin Poirier Theoret",
            "email": "mpoiriert@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "psr/log": "^3"
    },
    "require-dev": {
        "draw/dependency-injection": "^0.31",
        "draw/user-bundle": "^0.31",
        "monolog/monolog": "^3.5.0",
        "phpunit/phpunit": "^11.3",
        "symfony/event-dispatcher": "^6.4.0",
        "symfony/http-foundation": "^6.4.0",
        "symfony/http-kernel": "^6.4.0",
        "symfony/security-core": "^6.4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Component\\Log\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.31-dev"
        }
    }
}
