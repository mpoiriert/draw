<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults public="false" autoconfigure="true" autowire="true"/>
        <prototype namespace="Draw\Bundle\UserBundle\AccountLocker\Sonata\"
                   resource="../../AccountLocker/Sonata"
        />
        <prototype namespace="Draw\Bundle\UserBundle\AccountLocker\Sonata\Controller\"
                   resource="../../AccountLocker/Sonata/Controller">
            <tag name="controller.service_arguments"/>
        </prototype>
    </services>
</container>