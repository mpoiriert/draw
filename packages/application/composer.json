{
    "name": "draw/application",
    "description": "Component to handle application domain logic",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["symfony", "application", "configuration", "versioning"],
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.11",
        "doctrine/common": "^3.1",
        "doctrine/orm": "^2.11",
        "draw/contracts": "^1.0",
        "draw/core": "^1.0",
        "symfony/doctrine-bridge": "^5.4.3"
    },
    "require-dev": {
        "draw/tester-bundle": "^1.0",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Component\\Application\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
