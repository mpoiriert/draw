{
    "name": "draw/application",
    "description": "Component to handle application domain logic like configuration, cron, versioning",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["symfony", "application", "configuration", "cron", "versioning"],
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/common": "^3.1",
        "doctrine/orm": "^2.11",
        "draw/contracts": "^0.11",
        "draw/core": "^0.11",
        "symfony/doctrine-bridge": "^6.4.0",
        "symfony/event-dispatcher": "^6.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.0",
        "draw/tester": "^0.11",
        "symfony/cache": "^6.4.0",
        "symfony/console": "^6.4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Component\\Application\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.11-dev"
        }
    }
}
