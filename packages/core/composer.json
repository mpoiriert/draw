{
    "name": "draw/core",
    "description": "Core library for draw component and bundle.",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.4",
        "symfony/dependency-injection": "^4.4 || ^5.4",
        "symfony/property-access": "^4.4 || ^5.4",
        "symfony/validator": "^4.4 || ^5.4"
    },
    "require-dev": {
        "doctrine/annotations": "^1.11",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Component\\Core\\": ""
        },
        "files": [
            "ignore_annotations.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
