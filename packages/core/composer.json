{
    "name": "draw/core",
    "description": "Core library for draw component and bundle.",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=8.2",
        "symfony/dependency-injection": "^6.4.0",
        "symfony/property-access": "^6.4.0",
        "symfony/validator": "^6.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3 || ^12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Component\\Core\\": ""
        },
        "files": [
            "functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.37-dev"
        }
    }
}
