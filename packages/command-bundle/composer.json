{
    "name": "draw/command-bundle",
    "description": "This bundle track command execution in the database.",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["symfony", "command", "sonata"],
    "require": {
        "doctrine/dbal": "^3.3",
        "psr/log": "@stable",
        "sensiolabs/ansi-to-html": "^1.2",
        "symfony/config": "^5.4.3",
        "symfony/console": "^5.4.3",
        "symfony/dependency-injection": "^5.4.3",
        "symfony/expression-language": "^5.4.3"
    },
    "require-dev": {
        "doctrine/orm": "^2.11",
        "draw/tester": "^1.0",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "sonata-project/admin-bundle": "^4.8",
        "sonata-project/doctrine-orm-admin-bundle": "^4.2",
        "symfony/security-core": "^5.4.3"
    },
    "suggest": {
        "symfony/security-core": "If you want to user authentication in command line"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Bundle\\CommandBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
