{
    "name": "draw/aws-tool-kit-bundle",
    "description": "[BETA] This bundle offer a few AWS tool for symfony.",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["symfony", "aws"],
    "require": {
        "aws/aws-sdk-php": "^3.171",
        "symfony/console": "^5.4.3",
        "symfony/framework-bundle": "^5.4.3",
        "symfony/css-selector": "^5.4.3"
    },
    "require-dev": {
        "aws/aws-sdk-php-symfony": "^2.3",
        "draw/core": "^0.8",
        "draw/tester-bundle": "^0.8",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "sensio/framework-extra-bundle": "^5.6 || ^6.2",
        "symfony/http-client": "^5.4.3",
        "symfony/http-client-contracts": "^2.5",
        "symfony/yaml": "^5.4.3"
    },
    "conflict": {
        "guzzlehttp/guzzle": "<6"
    },
    "suggest": {
        "aws/aws-sdk-php-symfony": "To configure aws-sdk-php into symfony"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Bundle\\AwsToolKitBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
