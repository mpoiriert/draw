{
    "name": "draw/aws-tool-kit-bundle",
    "type": "symfony-bundle",
    "keywords": ["symfony", "aws"],
    "license": "MIT",
    "description": "[BETA] This bundle offer a few AWS tool for symfony.",
    "autoload": {
        "psr-4": {
            "Draw\\Bundle\\AwsToolKitBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "aws/aws-sdk-php": "^3.171",
        "symfony/console": "^4.4|^5.4",
        "symfony/framework-bundle": "^4.4|^5.4",
        "symfony/css-selector": "^4.4|^5.4"
    },
    "require-dev": {
        "draw/tester-bundle": "^0.7.52",
        "phpunit/phpunit": "^8.0|^9.0",
        "sensio/framework-extra-bundle": "^5.6|^6.2",
        "symfony/yaml": "^4.4|^5.4"
    },
    "conflict": {
        "guzzlehttp/guzzle": "<6"
    },
    "suggest": {
        "aws/aws-sdk-php-symfony": "To configure aws-sdk-php into symfony"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
