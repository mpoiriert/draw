{% if object is defined %}
    {% set object = object %}
{% else %}
    {% set object = null %}
{% endif %}
{% for action in item.action.actions %}
    <li>
        <a class="sonata-action-element" href="{{ action.generateUrl(admin, object) }}">
            {% if action.icon %}
                <i class="{{ action.icon }}" aria-hidden="true"></i>
            {% endif %}
            {{ action|translate_label }}
        </a>
    </li>
{% endfor %}
