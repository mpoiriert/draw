{% extends '@SonataAdmin/CRUD/base_show_field.html.twig' %}

{% block field %}
    <ul>
        {% for val in value %}
            <li>
                {% if field_description.options.entry_callback is defined %}
                    {{ field_description.options.entry_callback.call(val, val) | raw | spaceless }}
                {% else %}
                    {{ val | spaceless }}
                {% endif %}
            </li>
        {% endfor %}
    </ul>
{% endblock %}