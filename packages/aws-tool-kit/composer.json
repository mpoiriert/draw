{
    "name": "draw/aws-tool-kit",
    "description": "This package offer a few AWS tool kit.",
    "license": "MIT",
    "type": "library",
    "keywords": ["draw", "aws"],
    "require": {
        "aws/aws-sdk-php": "^3.171",
        "symfony/console": "^6.4.0",
        "symfony/event-dispatcher": "^6.4.0"
    },
    "require-dev": {
        "draw/core": "^0.11",
        "draw/tester": "^0.11",
        "phpunit/phpunit": "^9.0 || ^10.0",
        "symfony/cache": "^6.4.0",
        "symfony/http-client": "^6.4.0",
        "symfony/http-client-contracts": "^3.4"
    },
    "conflict": {
        "guzzlehttp/guzzle": "<6"
    },
    "suggest": {
        "aws/aws-sdk-php-symfony": "To configure aws-sdk-php into symfony"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Component\\AwsToolKit\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.11-dev"
        }
    }
}
