{
    "name": "draw/framework-extra-bundle",
    "description": "[WIP] Main bundle to configure draw component in symfony framework",
    "license": "MIT",
    "type": "library",
    "keywords": ["draw", "symfony", "framework", "bundle"],
    "authors": [
        {
            "name": "Martin Poirier Theoret",
            "email": "mpoiriert@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "draw/core": "^1.0",
        "symfony/config": "^5.4.3",
        "symfony/dependency-injection": "^5.4.3",
        "symfony/http-kernel": "^5.4.3"
    },
    "require-dev": {
        "draw/application": "^1.0",
        "draw/aws-tool-kit": "^1.0",
        "draw/console": "^1.0",
        "draw/contracts": "^1.0",
        "draw/log": "^1.0",
        "draw/mailer": "^1.0",
        "draw/messenger": "^1.0",
        "draw/process": "^1.0",
        "draw/security": "^1.0",
        "draw/tester": "^1.0",
        "draw/tester-bundle": "^1.0",
        "draw/user-bundle": "^1.0",
        "monolog/monolog": "^2.4.0",
        "pelago/emogrifier": "^6.0",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "psr/log": "@stable",
        "symfony/event-dispatcher": "^5.4.3",
        "symfony/http-foundation": "^5.4.3",
        "symfony/security-bundle": "^5.4.3",
        "symfony/security-core": "^5.4.3",
        "symfony/twig-bundle": "^5.4.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Bundle\\FrameworkExtraBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
