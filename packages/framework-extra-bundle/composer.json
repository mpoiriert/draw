{
    "name": "draw/framework-extra-bundle",
    "description": "[WIP] Main bundle to configure draw component in symfony framework",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "draw",
        "symfony",
        "framework",
        "bundle"
    ],
    "authors": [
        {
            "name": "Martin Poirier Theoret",
            "email": "mpoiriert@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "draw/core": "^0.37",
        "draw/dependency-injection": "^0.37",
        "symfony/config": "^6.4.0",
        "symfony/dependency-injection": "^6.4.0",
        "symfony/http-kernel": "^6.4.0"
    },
    "require-dev": {
        "draw/application": "^0.37",
        "draw/aws-tool-kit": "^0.37",
        "draw/console": "^0.37",
        "draw/contracts": "^0.37",
        "draw/cron-job": "^0.37",
        "draw/doctrine-extra": "^0.37",
        "draw/entity-migrator": "^0.37",
        "draw/log": "^0.37",
        "draw/mailer": "^0.37",
        "draw/messenger": "^0.37",
        "draw/open-api": "^0.37",
        "draw/process": "^0.37",
        "draw/sonata-integration-bundle": "^0.37",
        "draw/security": "^0.37",
        "draw/tester": "^0.37",
        "draw/tester-bundle": "^0.37",
        "draw/user-bundle": "^0.37",
        "draw/validator": "^0.37",
        "draw/workflow": "^0.37",
        "monolog/monolog": "^3.5.0",
        "pelago/emogrifier": "^7.0",
        "phpunit/phpunit": "^11.3 || ^12.0",
        "psr/log": "^3",
        "symfony/event-dispatcher": "^6.4.0",
        "symfony/http-foundation": "^6.4.0",
        "symfony/security-bundle": "^6.4.0",
        "symfony/security-core": "^6.4.0",
        "symfony/twig-bundle": "^6.4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Bundle\\FrameworkExtraBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.37-dev"
        }
    }
}
