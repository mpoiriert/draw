{
    "name": "draw/framework-extra-bundle",
    "description": "[WIP] Main bundle to configure draw component in symfony framework",
    "license": "MIT",
    "type": "library",
    "keywords": ["draw", "symfony", "framework", "bundle"],
    "authors": [
        {
            "name": "Martin Poirier Theoret",
            "email": "mpoiriert@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "symfony/config": "^4.4 || ^5.4",
        "symfony/dependency-injection": "^4.4 || ^5.4",
        "symfony/http-kernel": "^4.4 || ^5.4"
    },
    "require-dev": {
        "draw/contracts": "^0.8",
        "draw/process": "^0.8",
        "draw/tester-bundle": "^0.8",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Bundle\\FrameworkExtraBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
