{
    "name": "draw/open-api",
    "description": "Open Api PHP manipulation library. Only support v 2.0",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "jms/serializer": "^3.11",
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/event-dispatcher": "^6.4.0",
        "symfony/filesystem": "^6.4.0",
        "symfony/validator": "^6.4.0"
    },
    "require-dev": {
        "ext-zip": "*",
        "draw/core": "^0.38",
        "draw/dependency-injection": "^0.38",
        "draw/tester": "^0.38",
        "doctrine/common": "^3.1",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^3.6",
        "phpunit/phpunit": "^11.3 || ^12.0"
    },
    "conflict": {
        "draw/swagger": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Component\\OpenApi\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.38-dev"
        }
    }
}
