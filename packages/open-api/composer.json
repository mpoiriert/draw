{
    "name": "draw/open-api",
    "description": "Open Api PHP manipulation library. Only support v 2.0",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "jms/serializer": "^3.11",
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/validator": "^5.4.3"
    },
    "require-dev": {
        "doctrine/common": "^3.1",
        "doctrine/orm": "^2.11",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "conflict": {
        "draw/swagger": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Component\\OpenApi\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
