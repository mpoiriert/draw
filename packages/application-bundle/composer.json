{
    "name": "draw/application-bundle",
    "description": "Bundle to handle application domain logic",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["symfony", "application", "configuration"],
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/common": "^3.1",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.11",
        "draw/contracts": "^0.9",
        "draw/core": "^0.9",
        "draw/sonata-extra-bundle": "^1.0",
        "symfony/doctrine-bridge": "^5.4.3",
        "symfony/framework-bundle": "^5.4.3"
    },
    "require-dev": {
        "draw/tester-bundle": "^0.9",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "sonata-project/admin-bundle": "^4.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Bundle\\ApplicationBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9-dev"
        }
    }
}
