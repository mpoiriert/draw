{
    "name": "draw/security",
    "description": "Security related class compatible with symfony/security* component",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "draw",
        "symfony",
        "security"
    ],
    "authors": [
        {
            "name": "Martin Poirier Theoret",
            "email": "mpoiriert@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "draw/core": "^0.37",
        "draw/dependency-injection": "^0.37",
        "draw/framework-extra-bundle": "^0.37",
        "draw/messenger": "^0.37",
        "draw/tester": "^0.37",
        "firebase/php-jwt": "^6.1",
        "phpunit/phpunit": "^11.3 || ^12.0",
        "symfony/console": "^6.4.0",
        "symfony/messenger": "^6.4.0",
        "symfony/security-http": "^6.4.0",
        "symfony/security-core": "^6.4.0",
        "symfony/translation-contracts": "^2.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Component\\Security\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.37-dev"
        }
    }
}
