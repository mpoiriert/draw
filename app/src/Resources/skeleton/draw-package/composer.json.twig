{
  "name": "draw/{{ packageName }}",
  "description": "{{ packageDescription }}",
  "license": "MIT",
  "type": "library",
  "keywords": ["draw", "{{ type }}", "symfony"],
  "authors": [
    {
      "name": "Martin Poirier Theoret",
      "email": "mpoiriert@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "{{ namespace|replace({'\\':'\\\\'}) }}\\": ""
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.11-dev"
    }
  }
}