{
    "name": "draw/draw",
    "description": "Monolitic repository for draw components",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-zip": "*",
        "aws/aws-sdk-php": "^3.171",
        "doctrine/annotations": "^1.11",
        "doctrine/common": "^3.1",
        "doctrine/data-fixtures": "^1.5",
        "doctrine/dbal": "^3.3",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.11",
        "doctrine/persistence": "^2.2",
        "guzzlehttp/psr7": "^1.8 || ^2.0",
        "jms/serializer": "^3.11",
        "jms/serializer-bundle": "^3.7",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "psr/http-message": "^1.0",
        "psr/log": "@stable",
        "ramsey/uuid": "^4.2",
        "sensiolabs/ansi-to-html": "^1.2",
        "sonata-project/admin-bundle": "^4.8",
        "sonata-project/doctrine-orm-admin-bundle": "^4.2",
        "sonata-project/form-extensions": "^1.13",
        "symfony/browser-kit": "^5.4.3",
        "symfony/config": "^5.4.3",
        "symfony/console": "^5.4.3",
        "symfony/css-selector": "^5.4.3",
        "symfony/dependency-injection": "^5.4.3",
        "symfony/doctrine-bridge": "^5.4.3",
        "symfony/doctrine-messenger": "^5.4.3",
        "symfony/dom-crawler": "^5.4.3",
        "symfony/event-dispatcher": "^5.4.3",
        "symfony/expression-language": "^5.4.3",
        "symfony/filesystem": "^5.4.3",
        "symfony/framework-bundle": "^5.4.3",
        "symfony/http-foundation": "^5.4.3",
        "symfony/http-kernel": "^5.4.3",
        "symfony/mailer": "^5.4.3",
        "symfony/messenger": "^5.4.3",
        "symfony/process": "^5.4.3",
        "symfony/property-access": "^5.4.3",
        "symfony/runtime": "^5.4.3",
        "symfony/security-core": "^5.4.3",
        "symfony/security-guard": "^5.4.3",
        "symfony/security-http": "^5.4.3",
        "symfony/translation": "^5.4.3",
        "symfony/twig-bridge": "^5.4.3",
        "symfony/validator": "^5.4.3",
        "symfony/yaml": "^5.4.3",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "ext-pcntl": "*",
        "ext-zip": "*",
        "aws/aws-sdk-php-symfony": "^2.3",
        "bamarni/composer-bin-plugin": "^1.4",
        "composer/semver": "^3.2",
        "doctrine/data-fixtures": "^1.5",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/sql-formatter": "^1.1",
        "firebase/php-jwt": "^6.1",
        "monolog/monolog": "^2.4.0",
        "nelmio/cors-bundle": "^2.0",
        "pdeans/http": "^1.1",
        "pelago/emogrifier": "^6.0",
        "scheb/2fa-bundle": "^5.12",
        "scheb/2fa-qr-code": "^5.12",
        "scheb/2fa-totp": "^5.12",
        "sensio/framework-extra-bundle": "^5.6 || ^6.2",
        "sonata-project/admin-bundle": "^4.8",
        "sonata-project/doctrine-orm-admin-bundle": "^4.2",
        "symfony/cache": "^5.4.3",
        "symfony/debug-bundle": "^5.4.3",
        "symfony/dependency-injection": "^5.4.3",
        "symfony/dotenv": "^5.4.3",
        "symfony/event-dispatcher": "^5.4.3",
        "symfony/expression-language": "^5.4.3",
        "symfony/flex": "^1.18",
        "symfony/form": "^5.4.3",
        "symfony/http-client": "^5.4.3",
        "symfony/http-client-contracts": "^2.5",
        "symfony/monolog-bundle": "^3.7",
        "symfony/phpunit-bridge": "^5.4.3",
        "symfony/security-bundle": "^5.4.3",
        "symfony/stopwatch": "^5.4.3",
        "symfony/translation-contracts": "^2.5",
        "symfony/twig-bundle": "^5.4.3",
        "symfony/web-profiler-bundle": "^5.4.3",
        "terminal42/service-annotation-bundle": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.7",
        "phpstan/phpstan-symfony": "^1.2",
        "phpstan/phpstan-phpunit": "^1.1",
        "scheb/2fa-email": "^5.12"
    },
    "replace": {
        "draw/application": "self.version",
        "draw/aws-tool-kit": "self.version",
        "draw/console": "self.version",
        "draw/contracts": "self.version",
        "draw/core": "self.version",
        "draw/doctrine-extra": "self.version",
        "draw/framework-extra-bundle": "self.version",
        "draw/log": "self.version",
        "draw/mailer": "self.version",
        "draw/messenger": "self.version",
        "draw/open-api": "self.version",
        "draw/open-api-bundle": "self.version",
        "draw/process": "self.version",
        "draw/profiling": "self.version",
        "draw/security": "self.version",
        "draw/sonata-extra-bundle": "self.version",
        "draw/sonata-integration-bundle": "self.version",
        "draw/tester": "self.version",
        "draw/tester-bundle": "self.version",
        "draw/user-bundle": "self.version",
        "draw/validator": "self.version",
        "symfony/polyfill-php54": "1.99",
        "symfony/polyfill-php55": "1.99",
        "symfony/polyfill-php56": "1.99",
        "symfony/polyfill-php70": "1.99",
        "symfony/polyfill-php71": "1.99",
        "symfony/polyfill-php72": "1.99",
        "symfony/polyfill-php73": "1.99"
    },
    "conflict": {
        "doctrine/data-fixtures": "<1.5.2",
        "guzzlehttp/guzzle": "<6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Bundle\\FrameworkExtraBundle\\": "packages/framework-extra-bundle/",
            "Draw\\Bundle\\SonataExtraBundle\\": "packages/sonata-extra-bundle/",
            "Draw\\Bundle\\SonataIntegrationBundle\\": "packages/sonata-integration-bundle/",
            "Draw\\Bundle\\TesterBundle\\": "packages/tester-bundle/",
            "Draw\\Bundle\\UserBundle\\": "packages/user-bundle/",
            "Draw\\Component\\Application\\": "packages/application/",
            "Draw\\Component\\AwsToolKit\\": "packages/aws-tool-kit/",
            "Draw\\Component\\Console\\": "packages/console/",
            "Draw\\Component\\Core\\": "packages/core/",
            "Draw\\Component\\Log\\": "packages/log/",
            "Draw\\Component\\Mailer\\": "packages/mailer/",
            "Draw\\Component\\Messenger\\": "packages/messenger/",
            "Draw\\Component\\OpenApi\\": "packages/open-api/",
            "Draw\\Component\\Process\\": "packages/process/",
            "Draw\\Component\\Profiling\\": "packages/profiling/",
            "Draw\\Component\\Security\\": "packages/security/",
            "Draw\\Component\\Tester\\": "packages/tester/",
            "Draw\\Component\\Validator\\": "packages/validator/",
            "Draw\\Contracts\\": "packages/contracts/",
            "Draw\\DoctrineExtra\\": "packages/doctrine-extra/"
        },
        "files": [
            "packages/core/functions.php",
            "packages/core/ignore_annotations.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/src/",
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9-dev"
        },
        "src-dir": "app/src"
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts",
            "@composer bin all install --ansi"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "draw:open-api:install-sandbox %PUBLIC_DIR%/open-api/sandbox": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
