{
    "name": "draw/draw",
    "description": "Monolitic repository for draw components",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Poirier-Theoret",
            "email": "mpoiriert@gmail.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Draw\\": "src//"
        }
    },
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "symfony/property-access": "^3.0|^2.8",
        "guzzlehttp/psr7": "^1.4",
        "pdeans/http": "^1.1",
        "symfony/http-foundation": "^4.3",
        "symfony/browser-kit": "^4.3",
        "symfony/console": "^4.3",
        "symfony/framework-bundle": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/yaml": "^4.3",
        "symfony/flex": "^1.4",
        "symfony/orm-pack": "^1.0",
        "aws/aws-sdk-php": "^3.112",
        "symfony/mailer": "^4.3",
        "symfony/security": "^4.3",
        "symfony/expression-language": "^4.3",
        "sensiolabs/ansi-to-html": "^1.1"
    },
    "replace": {
        "draw/aws-tool-kit-bundle": "self.version",
        "draw/cron-bundle": "self.version",
        "draw/tester-bundle": "self.version",
        "draw/profiling": "self.version",
        "draw/tester": "self.version"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
