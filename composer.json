        "draw/aws-tool-kit": "self.version",
        "draw/console": "self.version",
        "draw/contracts": "self.version",
        "draw/core": "self.version",
        "draw/cron-job": "self.version",
        "draw/data-synchronizer": "self.version",
        "draw/dependency-injection": "self.version",
        "draw/doctrine-extra": "self.version",
        "draw/entity-migrator": "self.version",
        "draw/fixer": "self.version",
        "draw/framework-extra-bundle": "self.version",
        "draw/graphviz": "self.version",
        "draw/log": "self.version",
        "draw/mailer": "self.version",
        "draw/messenger": "self.version",
        "draw/open-api": "self.version",
        "draw/open-api-bundle": "self.version",
        "draw/process": "self.version",
        "draw/profiling": "self.version",
        "draw/security": "self.version",
        "draw/sonata-extra-bundle": "self.version",
        "draw/sonata-import-bundle": "self.version",
        "draw/sonata-integration-bundle": "self.version",
        "draw/tester": "self.version",
        "draw/tester-bundle": "self.version",
